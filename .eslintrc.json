{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/prop-types": "off",
        "max-len": ["error",{
            "code": 160
        }],
        "import/prefer-default-export": "off",
        "no-underscore-dangle": "off",
        "no-console":"off"
    }
}
